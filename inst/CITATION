citHeader("To cite bspmma in publications use:")

citEntry(entry = "Article",
  title        = "{bspmma}: An {R} Package for Bayesian Semiparametric Models for Meta-Analysis",
  author       = personList(as.person("Deborah Burr")),
  journal      = "Journal of Statistical Software",
  year         = "2012",
  volume       = "50",
  number       = "4",
  pages        = "1--23",
  url          = "http://www.jstatsoft.org/v50/i04/",

  textVersion  =
  paste("Deborah Burr (2012).",
        "bspmma: An R Package for Bayesian Semiparametric Models for Meta-Analysis.",
        "Journal of Statistical Software, 50(4), 1-23.",
        "URL http://www.jstatsoft.org/v50/i04/.")
)
